<script type="text/javascript">
    function JoinWebinar_OnClick() {
        window.location.href = "https://attendee.gotowebinar.com/register/8117689235671252225";
    }

    function addSourceToVideo(element, src) {
        var source = document.createElement('source');
        source.src = src;
        source.type = "video/mp4";

        element.appendChild(source);
        element.load();
        element.play();
    }

    $(document).ready(function () {
        var videoElement = document.getElementById("video");
        addSourceToVideo(videoElement, "https://mediasvcmg26x4p1t4vmz.blob.core.windows.net/asset-e7b6a189-cd0f-4409-a949-56ea9cbec788/2017-09-17 20.00 Intro to Magenta Webinar 8pm ET Sunday.mp4?sv=2012-02-12&sr=c&si=926e9b73-18bf-4b21-a059-9c9db959a3c3&sig=fAxsRebneXZDN94GZqhBQKiZkYY0E%2Fd%2FHUUoJrd3Zfs%3D&se=2018-09-18T10%3A12%3A18Z");
