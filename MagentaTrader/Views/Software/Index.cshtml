            var verified = false;
            switch(order.type) {
                case "market":
                    verified = true;
                    break;
                case "limit":
                    if (order.price > 0) verified = true;
                    break;
                case "stop":
                    if (order.stop > 0) verified = true;
                    break;
                case "stop_limit":
                    if (order.price > 0 && order.stop > 0) verified = true;
                    break;
                default:
                    verified = false;
                    break;
            }

            if (verified == true) {
                var data = JSON.stringify(order);

