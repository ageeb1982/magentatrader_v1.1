    function GetWebinarData(dayDate) {
        var webinars = "";
        for (var i = 0; i < WebinarData.length; i++) {
            var webinarDate = new Date(WebinarData[i]["EventDate"]);
            if (parseDate(dayDate).equals(webinarDate) == true) {
                webinars = webinars + "<a href='" + WebinarData[i]["URL"] + "'>" + WebinarData[i]["EventDescription"] + "</a><br />"
            }
        }
        return webinars;
    }
    function parseDate(input) {
        var parts = input.split('-');
        return new Date(parts[0], parts[1] - 1, parts[2]);
    }

    function CmdGetWebinar_OnClick() {
        var m = parseInt($("#WebinarMonth").val());
        var y = parseInt($("#WebinarYear").val());

        MakeCalendarGrid(m, y);
