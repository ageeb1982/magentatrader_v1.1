    $(document).ready(function () {
        CalendarGrid = new wijmo.grid.FlexGrid('#CalendarGrid');

        var d = new Date();
        var m = d.getMonth();
        var y = d.getFullYear();

        $("#WebinarMonth").val(m);
        $("#WebinarYear").val(y);

        $.ajax({
            url: '/api/LatestEvent',
            cache: false,
            type: 'GET',
            contentType: 'application/json; charset=utf-8',
            data: {},
            success: function (Results) {
                while (WebinarData.length > 0) WebinarData.pop();
                WebinarData = Results;
                MakeCalendarGrid(m, y);
            }
        }).fail(
            function (xhr, textStatus, err) {  }
        );
    });
